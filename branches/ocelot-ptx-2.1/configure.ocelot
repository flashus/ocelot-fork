{
	ocelot: "ocelot",
	trace: { 
		memoryChecker: true,
		raceDetector: {
			enabled: true,
			ignoreIrrelevantWrites: true
		},
		debugger: {
			enabled: false,
			kernelFilter: "_Z13scalarProdGPUPfS_S_ii",
			alwaysAttach: false
		}
	},
	cuda: {
		implementation: CudaRuntime,
		runtimeApiTrace: "trace/CudaAPI.trace"
	},
	executive: {
		devices: [ llvm ],
		optimizationLevel: none,
		workerThreadLimit: 1
	}
}

