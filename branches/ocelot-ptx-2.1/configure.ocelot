{
	ocelot: "ocelot",
	version: "1.1.520",
	trace: { 
		memoryChecker: false,
		raceDetector: false,
		debugger: {
			enabled: true,
			kernelFilter: "sequence",
			alwaysAttach: false
		}
	},
	cuda: {
		implementation: CudaRuntime,
		runtimeApiTrace: "traces/CudaAPI.trace"
	},
	executive: {
		devices: [ emulated ],
		optimizationLevel: basic,
		workerThreadLimit: 1
	}
}

