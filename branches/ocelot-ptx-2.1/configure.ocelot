{
	ocelot: "ocelot",
	version: "1.1.520",
	trace: { 
		database: "traces/database.trace",
		memory: false,
		branch: false,
		sharedComputation: false,
		instruction: false,
		parallelism: false,
		cacheSimulator: false,
		memoryChecker: true,
		raceDetector: true
	},
	cuda: {
		implementation: "CudaRuntime",
		runtimeApiTrace: "trace/CudaAPI.trace"
	},
	executive: {
		devices: [ llvm, emulated, nvidia ],
		optimizationLevel: basic,
		workerThreadLimit: 1
	}
}

