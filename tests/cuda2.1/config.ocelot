<ocelot>
	<OcelotRuntime>
		<TraceDatabase>
			"traces/database.trace"
		</TraceDatabase>
		<UseInPlaceTraces>
			1
		</UseInPlaceTraces>
		<UseMemoryTraceGenerator>
			1
		</UseMemoryTraceGenerator>
		<UseBranchTraceGenerator>
			1
		</UseBranchTraceGenerator>
		<UseSharedComputationGenerator>
			1
		</UseSharedComputationGenerator>
		<UseParallelismTraceGenerator>
			1
		</UseParallelismTraceGenerator>
	</OcelotRuntime>
	<CudaRuntime>
		<CudaRuntimeImplementation>
			TraceGeneratingCudaRuntime
		</CudaRuntimeImplementation>
		<TraceGeneratingCudaRuntime>
			<CudaRuntimeApiTraceFile>
				"traces/CudaApi.trace"
			</CudaRuntimeApiTraceFile>
		</TraceGeneratingCudaRuntime>
	</CudaRuntime>
</ocelot>
