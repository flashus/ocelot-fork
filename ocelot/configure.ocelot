{
	ocelot: "ocelot",
	trace: { 
		enabled: true,
		database: "traces/database.trace",
		memoryChecker: true,
		raceDetector: {
			enabled: true,
			ignoreIrrelevantWrites: true
		},
		debugger: {
			enabled: false,
			kernelFilter: "_Z13scalarProdGPUPfS_S_ii",
			alwaysAttach: true
		},
		performanceBound: {
			enabled: false,
			protocol: sm_20,
			render: false,
			output: csv
		},
		cacheSimulator: {
			enabled: false,
			writebackTime: 50,
			cacheSize: 8192,
			lineSize: 64,
			hitTime: 1,
			missTime: 200,
			associativity: 1,
			instructionMemory: true
		},
		controlFlowVisualizer: {
			enabled: false,
			allInstructions: false
		}
	},
	cuda: {
		implementation: CudaRuntime,
		runtimeApiTrace: "trace/CudaAPI.trace"
	},
	executive: {
		devices: [ nvidia, emulated, llvm, amd ],
		optimizationLevel: basic,
		workerThreadLimit: 2
	}
}

